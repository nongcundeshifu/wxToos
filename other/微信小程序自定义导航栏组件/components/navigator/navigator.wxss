/* components/navigator/navigator.wxss */
/* 这个会只对组件内的元素生效  */
navigator-view {
    height: 100%;
}
.page {
    height: 100%;
    box-sizing: border-box;
    overflow-y: hidden;
}

button:after {
    border: none;
}
button-hover {
    background-color: rgba(0, 0, 0, 0.2); opacity: 0.7;
}
.navigate {
	box-sizing: border-box;
	width: 100%;
    text-align: center;
    position: relative;
    
}
.navigate > view {
    width: 100%；
}
.navigate .title {
    width: 300rpx;
    font-size: 34rpx;
}
.navigate .both-sides {
	height: 100%;
}
.navigate .both-sides text {
    font-size: 28rpx;
}
.navigate .both-sides .navTap {
    height: 100%;
    padding: 0 30rpx;
    background: rgba(0,0,0,0);
    border:none;
    margin: 0;
}


/*下拉刷新模式*/
.refresh.page {
    
}
.refresh .navigate{
    position: fixed; 
    top: 0;
    left: 0;
    z-index: 9;
}
